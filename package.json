{
  "name": "node-express-type-kit",
  "version": "1.0.0",
  "description": "use node/express in typescript ",
  "author": "Priyanshu Rathore",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:live",
    "build": "npm run build-ts && npm run tslint",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/server.ts",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "compile": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.10.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.18",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.3.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/mongodb": "^3.1.22",
    "@types/mongoose": "^5.3.21",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.0",
    "@types/passport": "^1.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.2",
    "@types/winston": "^2.3.7",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
